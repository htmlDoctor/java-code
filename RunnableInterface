public class ThreadRunnableEx implements Runnable{
	public void run(){
		
		for(int i=0;i<=10;i++){
			
		
			System.out.println("The number is "+i);
		}
		
	}

	public static void main(String[] args) {
		ThreadRunnableEx ex =new ThreadRunnableEx();
		Thread t1 = new Thread(ex);
		
		t1.start();

	}

}
